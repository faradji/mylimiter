{
  "name": "mylimiter",
  "version": "2.0.2",
  "description": "express middleware for limiting the number of requests over a given time period using MySQL",
  "main": "index.js",
  "scripts": {
    "pretest": "jshint *.js ; cp -n test/db.conf.example test/db.conf || true",
    "test": "mocha -R spec test/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tcort/mylimiter"
  },
  "keywords": [
    "express",
    "rate",
    "limiter",
    "middleware",
    "mysql"
  ],
  "author": "Thomas Cort <linuxgeek@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tcort/mylimiter/issues"
  },
  "homepage": "https://github.com/tcort/mylimiter",
  "dependencies": {
    "mysql": "^2.9.0",
    "ssi-logger": "^2.0.0"
  },
  "devDependencies": {
    "dbtestutil": "^2.0.1",
    "express": "^4.13.3",
    "jshint": "^2.8.0",
    "mocha": "^2.3.4",
    "nconf": "^0.8.2",
    "supertest": "^1.1.0"
  },
  "jshintConfig": {
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "globalstrict": true,
    "immed": true,
    "indent": 4,
    "moz": true,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "trailing": true,
    "undef": true,
    "smarttabs": true,
    "strict": true,
    "validthis": true,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false
    }
  }
}
